<div class="container mt-5">
    <h1>User Profile</h1>
    <table class="table">
        <tbody>
            <tr>
                <th scope="row">User ID</th>
                <td>{{id}}</td>
            </tr>
            <tr>
                <th scope="row">Username</th>
                <td>{{username}}</td>
            </tr>
            <tr>
                <th scope="row">Created At</th>
                <td>{{created_at}}</td>
            </tr>
            <tr>
                <th scope="row">Last Login</th>
                <td>{{last_login}}</td>
            </tr>
        </tbody>
    </table>
    <a href="/home" class="btn btn-primary">Back to Home</a>
</div>
